[user]
  name = Alan Yeo Jie Wei
  email = yeojw10@gmail.com
[push]
  default = simple
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  hgp = show --format=\"From: %an <%ae>%n%s%n%b\" -U8
  co = checkout
  st = status -sb
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
[mergetool]
  keepbackup = false
